{
   "background": {
      "scripts": [ "jquery-2.0.0.js", "granted_background.js" ]
   },
   "content_scripts": [ {
      "css": [ "granted.css" ],
      "js": [ "jquery-2.0.0.js", "granted.js"],
      "matches": [ "<all_urls>" ]
   } ],
   "description": "Puts a download link to Songsterr tabs",
   "icons": {
      "128": "icon128.png",
      "16": "icon16.png",
      "48": "icon48.png"
   },
   "manifest_version": 3,
   "name": "Ripsterr",
   "permissions": [ "tabs", "http://*/*", "notifications" ],
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "1.0.1",
   "browser_action": {
       "default_popup": "popup.html"
   }
}
